alt + Return
	kitty

control + @space
	rofi -show run

super + shift + Tab
  rofi -show window

alt + Escape
	pkill -USR1 -x sxhkd

alt + {_,shift + }{h,n,e,i}
	bspc node -{f,s} {west,south,north,east}

control + alt + {h,i}
	bspc desktop -f {prev,next}.local

alt + f
	bspc desktop -l next

alt + {t, shift + t}
	bspc node -t {tiled,floating}

XF86MonBrightnessDown
 light -U 5

XF86MonBrightnessUp
 light -A 5
